	<parameter name="listid" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
b.number,
b.id_apiary,
b.number,
date(b.date/1000,'unixepoch') ,
 b.type,
 a.name AS apiary_name
FROM apiaries a, beehives b
